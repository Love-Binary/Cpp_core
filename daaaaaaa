PROGRAM-01(b):Codeand analyse solutiontoKnapSack problem usingdynamicapproach.
#include<iostream>
using namespace std;
int max(int a,int b)
{
	if(a>b)
	return a;
	else
	return b;
}
int main()
{
int B[50][50],W[50],b[50],m,n,i,w;
cout<<"\n Enter the number of objects:";
cin>>n;
cout<<"\n Enter the knapsack capacity:";
cin>>m;
cout<<"\n  Enter the benefits of objects:\n";
for(i=1;i<=n;i++)
cin>>b[i];
cout<<"\n  Enter the weights of objects:\n";
for(i=1;i<=n;i++)
cin>>W[i];
for(i=0;i<n;i++)
B[i][0]=0;
for(w=0;w<m;w++)
B[0][w]=0;
for(i=1;i<=n;i++)
{
	for(w=1;w<=m;w++)
	{
	   if(W[i]<=w)	
		B[i][w]=max(B[i-1][w],(b[i]+B[i-1][w-W[i]]));
	   else
		B[i][w]=B[i-1][w];
	}
}
cout<<"\n  Maximum benefit is "<<B[n][m];
return 0;
}